      "/professionals": {
        "get": {
          "tags": [
            "DB連携(DBLinkageApi)"
          ],
          "summary": "DB連携",
          "description": "パラメータ(法人番号,個人Gビズ内部ID)と、GビズIDで認証した情報との同一性を確認し、一致しない場合は認証のエラーを返却する。",
          "operationId": "getProfessional",
          "parameters": [
            {
              "name": "corpid",
              "in": "query",
              "description": "法人番号",
              "required": false,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "gbizid",
              "in": "query",
              "description": "個人Gビズ内部ID",
              "required": false,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Hojindata"
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request"
            },
            "404": {
              "description": "Not Found"
            }
          }
        }
      },
      "/reception_jizen": {
        "post": {
          "tags": [
            "受付・要求API(ListenApi)"
          ],
          "summary": "事前同意受リクエスト受付・要求API",
          "description": "事前同意のあるデータ提供システムのAPIを呼び出します。",
          "operationId": "receptJizen",
          "requestBody": {
            "content": {
              "application/x-www-form-urlencoded": {
                "schema": {
                  "type": "object",
                  "required": [
                    "call_api",
                    "method"
                  ],
                  "properties": {
                    "method": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのAPIのメソッド",
                      "enum": [
                        "GET",
                        "POST",
                        "PUT",
                        "PATCH"
                      ]
                    },
                    "call_api": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのAPIのエンドポイント。クライアントIDを指定する場合はパスのみ。<br>（例）<br>クライアントIDを指定しない場合: https://node.example.com/v1/corporations<br>クライアントIDを指定する場合: /v1/corporations"
                    },
                    "client_id": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのクライアントID。"
                    },
                    "header": {
                      "type": "array",
                      "description": "呼び出すデータ提供システムのAPIの呼び出しに必要なヘッダ。複数のヘッダを指定する場合、次のように指定。<br>header=accept: application/json&header=content-type: application/json",
                      "items": {
                        "type": "string",
                        "enum": [
                          "Accept: application/json",
                          "Content-Type: application/json"
                        ]
                      }
                    },
                    "scope": {
                      "type": "string",
                      "description": "取得する法人データのスコープを指定。複数指定の場合、半角スペース区切り。指定しない場合、全スコープを指定したとみなす。スコープは以下。<br>スコープ：説明<br>basic：法人データの「Basic」の値<br>application：法人データの「Application」の値<br>finance：法人データの「Finance」の値<br>officer_list：法人データの「Officer list」の値<br>shareholder_list：法人データの「Shareholder list」の値<br>office_list：法人データの「Office list」の値<br>list_of_professions：法人データの「List of professions」の値<br>certificate：法人データの「Certificate」の値",
                      "enum": [
                        "basic",
                        "application",
                        "finance",
                        "officer_list",
                        "shareholder_list",
                        "office_list",
                        "list_of_professions",
                        "certificate"
                      ]
                    },
                    "body": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのAPIの呼び出しに必要なJSON"
                    }
                  }
                }
              }
            },
            "required": true
          },
          "responses": {
            "default": {
              "description": "呼び出すデータ提供システムのAPI仕様のレスポンス参照"
            }
          },
          "security": [
            {
              "BasicAuth": [
                
              ]
            }
          ]
        }
      },
      "/reception_tsudo": {
        "post": {
          "tags": [
            "受付・要求API(ListenApi)"
          ],
          "summary": "都度同意受リクエスト受付・要求API",
          "description": "都度同意でユーザの同意を得たスコープでデータ提供システムのAPIを呼び出します。",
          "operationId": "receptTsudo",
          "requestBody": {
            "content": {
              "application/x-www-form-urlencoded": {
                "schema": {
                  "type": "object",
                  "required": [
                    "call_api",
                    "method",
                    "nonce"
                  ],
                  "properties": {
                    "method": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのAPIのメソッド",
                      "enum": [
                        "GET",
                        "POST",
                        "PUT",
                        "PATCH"
                      ]
                    },
                    "call_api": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのAPIのURL。クライアントIDを指定する場合はパスのみ。<br>（例）<br>クライアントIDが無い場合: https://node.example.com/v1/corporations<br>クライアントIDが有る場合: /v1/corporations"
                    },
                    "client_id ": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムAPIのクライアントID。"
                    },
                    "header": {
                      "type": "array",
                      "description": "呼び出すデータ提供システムのAPIの呼び出しに必要なヘッダ。複数のヘッダを指定する場合、次のように指定。<br>header=accept: application/json&header=content-type: application/json",
                      "items": {
                        "type": "string",
                        "enum": [
                          "Accept: application/json",
                          "Content-Type: application/json"
                        ]
                      }
                    },
                    "nonce": {
                      "type": "string",
                      "description": "都度同意後のリダイレクトURLのクエリーパラメータnonceの値を指定"
                    },
                    "body": {
                      "type": "string",
                      "description": "呼び出すデータ提供システムのAPIの呼び出しに必要なJSON"
                    }
                  }
                }
              }
            },
            "required": true
          },
          "responses": {
            "default": {
              "description": "呼び出すデータ提供システムのAPI仕様のレスポンス参照"
            }
          },
          "security": [
            {
              "BasicAuth": [
                
              ]
            }
          ]
        }
      },
      "/scope.html": {
        "get": {
          "tags": [
            "スコープ選択画面(ScopeApi)"
          ],
          "summary": "スコープ選択画面取得",
          "description": "ユーザの同意を得た場合、gBizConnect Node導入時に設定した呼び出し側システムのURLにリダイレクトします。リダイレクトURLのクエリーパラメータstateの値がスコープ選択画面リクエスト時に指定したstateの値と一致することをチェックしてください。",
          "operationId": "selectScope",
          "parameters": [
            {
              "name": "state",
              "in": "query",
              "description": "呼び出し側システムで生成するランダム文字列",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            }
          }
        }
      }
    },
