          "summary": "証明書・通知書等番号一覧取得",
          "description": "法人番号で指定された法人の証明書・通知書等の通知番号一覧を取得します。",
          "operationId": "getNotifications",
          "parameters": [
            {
              "name": "corporate-number",
              "in": "path",
              "description": "法人番号",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/NotificationNumberArray"
                  }
                }
              }
            },
            "404": {
              "description": "Not Found"
            }
          }
        },
        "post": {
          "tags": [
            "証明書・通知書等(NotificationsApi)"
          ],
          "summary": "証明書・通知書等登録",
          "description": "法人番号で指定された法人の証明書・通知書等を登録します。",
          "operationId": "createNotification",
          "parameters": [
            {
              "name": "corporate-number",
              "in": "path",
              "description": "法人番号",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "description": "証明書・通知書等",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Shomeisho"
                }
              }
            },
            "required": true
          },
          "responses": {
            "201": {
              "description": "Created",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Shomeisho"
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request"
            }
          }
        }
      },
      "/corporations/{corporate-number}/notifications/{notification-number}": {
        "get": {
          "tags": [
            "証明書・通知書等(NotificationsApi)"
          ],
          "summary": "証明書・通知書等取得",
          "description": "法人番号と通知番号で指定された証明書・通知書等を取得します。",
          "operationId": "getNotification",
          "parameters": [
            {
              "name": "corporate-number",
