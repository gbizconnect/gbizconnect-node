{
    "openapi": "3.0.1",
    "info": {
      "title": "gBizConnect Node API",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "https://localhost/jsonserver/v1"
      }
    ],
    "tags": [
      {
        "name": "法人データ(HojindataApi)",
        "description": "データ提供システムに事前登録されている法人データの取得・置換・部分更新"
      },
      {
        "name": "証明書・通知書等(NotificationsApi)",
        "description": "証明書・通知書等の取得・登録"
      },
      {
        "name": "DB連携(DBLinkageApi)",
        "description": "DB連携"
      },
      {
        "name": "受付・要求API(ListenApi)",
        "description": "gBizConnect Nodeを導入したシステムがデータ提供システムの公開するAPIを呼び出す際にリクエストを受け付ける"
      },
      {
        "name": "スコープ選択画面(ScopeApi)",
        "description": "都度同意でユーザがスコープを選択する画面"
      }
    ],
    "paths": {
      "/corporations": {
        "get": {
          "tags": [
            "法人データ(HojindataApi)"
          ],
          "summary": "法人番号一覧取得",
          "description": "法人番号の一覧を取得します。",
          "operationId": "getCorporations",
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/CorporateNumberArray"
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request"
            },
            "404": {
              "description": "Not Found"
            }
          }
        }
      },
      "/corporations/{corporate-number}": {
        "get": {
          "tags": [
            "法人データ(HojindataApi)"
          ],
          "summary": "法人データ取得",
          "description": "法人番号で指定された法人の法人データを、事前同意受付・要求APIのパラメータに渡されたスコープに基づいて取得します。",
          "operationId": "getCorporationByCorporateNumber",
          "parameters": [
            {
              "name": "corporate-number",
              "in": "path",
              "description": "法人番号",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "スコープに基づいた法人データ",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Hojindata"
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request"
            },
            "404": {
              "description": "Not Found"
            }
          }
        },
        "put": {
          "tags": [
            "法人データ(HojindataApi)"
          ],
          "summary": "法人データ置換",
          "description": "法人番号で指定された法人の法人データを置換します。置換はリクエストボディで渡された法人データで既存の法人データを置き換えるため、変更がない項目も全て含めてください。",
          "operationId": "putCorporation",
          "parameters": [
            {
              "name": "corporate-number",
              "in": "path",
              "description": "法人番号",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "description": "法人データ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Hojindata"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Hojindata"
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request"
            },
            "404": {
              "description": "Not Found"
            }
          }
        },
        "patch": {
          "tags": [
            "法人データ(HojindataApi)"
          ],
          "summary": "法人データ部分更新",
          "description": "法人番号で指定された法人の法人データを部分的に更新します。部分更新はリクエストボディで渡された法人データに含まれる項目だけを既存の法人データに反映します。変更がない項目を含める必要はありません。",
          "operationId": "patchCorporation",
          "parameters": [
            {
              "name": "corporate-number",
              "in": "path",
              "description": "法人番号",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "description": "法人データ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Hojindata"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Hojindata"
                  }
                }
              }
            },
            "400": {
              "description": "Bad Request"
            },
            "404": {
              "description": "Not Found"
            }
          }
        }
      },
      "/corporations/{corporate-number}/notifications": {
        "get": {
          "tags": [
            "証明書・通知書等(NotificationsApi)"
